{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "no-var-keyword": false,
    "prefer-const": false,
    "member-access": false,
    "semicolon": false,
    "quotemark": [true, "single"],
    "no-implicit-dependencies": false,
    "object-literal-sort-keys": false,
    "no-console": false
  }
}
